<style>
  .comparison-table-section {
    padding: {{ section.settings.section_padding_top }}px 20px {{ section.settings.section_padding_bottom }}px;
    background-color: {{ section.settings.bg_color }};
  }

  .comparison-table-section .container {
    max-width: {{ section.settings.container_width }}px;
    margin: 0 auto;
  }

  .comparison-table-section .section-heading {
    text-align: center;
    font-size: {{ section.settings.heading_size }}px;
    color: {{ section.settings.heading_color }};
    margin-bottom: {{ section.settings.heading_margin }}px;
    font-weight: {{ section.settings.heading_weight }};
  }

  .comparison-table {
    width: 100%;
    border-collapse: separate;
    border-spacing: 0;
    overflow: hidden;
  }

  .comparison-table thead th {
    padding: {{ section.settings.header_padding }}px;
    text-align: center;
    border-bottom: 2px solid {{ section.settings.border_color }};
    background-color: {{ section.settings.header_bg_color }};
    font-size: {{ section.settings.header_font_size }}px;
    font-weight: {{ section.settings.header_font_weight }};
    color: {{ section.settings.header_text_color }};
  }

  .comparison-table thead th:first-child {
    text-align: left;
    width: 25%;
  }

  .comparison-table thead th.brand-column {
    padding: {{ section.settings.brand_column_padding }}px;
  }

  .comparison-table thead th.brand-column img {
    max-width: {{ section.settings.brand_logo_width }}px;
    height: auto;
  }

  .comparison-table tbody tr {
    border-bottom: 1px solid {{ section.settings.border_color }};
  }

  .comparison-table tbody tr:hover {
    background-color: {{ section.settings.row_hover_color }};
  }

  .comparison-table tbody td {
    padding: {{ section.settings.cell_padding }}px;
    text-align: center;
    vertical-align: middle;
    font-size: {{ section.settings.cell_font_size }}px;
    color: {{ section.settings.cell_text_color }};
  }

  .comparison-table tbody td:first-child {
    text-align: left;
    font-weight: {{ section.settings.feature_name_weight }};
    color: {{ section.settings.feature_name_color }};
  }

  .comparison-table .product-icon img {
    max-width: {{ section.settings.product_icon_size }}px;
    height: auto;
    display: inline-block;
  }

  .comparison-table .checkmark {
    display: inline-flex;
    align-items: center;
    justify-content: center;
    width: {{ section.settings.icon_size }}px;
    height: {{ section.settings.icon_size }}px;
    background-color: {{ section.settings.checkmark_bg_color }};
    border-radius: 50%;
  }

  .comparison-table .checkmark svg {
    width: 60%;
    height: 60%;
    fill: {{ section.settings.checkmark_icon_color }};
  }

  .comparison-table .cross {
    display: inline-flex;
    align-items: center;
    justify-content: center;
    width: {{ section.settings.icon_size }}px;
    height: {{ section.settings.icon_size }}px;
    background-color: {{ section.settings.cross_bg_color }};
    border-radius: 50%;
  }

  .comparison-table .cross svg {
    width: 60%;
    height: 60%;
    fill: {{ section.settings.cross_icon_color }};
  }

  .comparison-table .price-value {
    font-weight: 600;
    font-size: {{ section.settings.price_font_size }}px;
  }

  .comparison-table .price-detail {
    font-size: {{ section.settings.price_detail_font_size }}px;
    display: block;
    margin-top: 4px;
  }

  .comparison-table .absorption-time {
    display: block;
    font-size: {{ section.settings.absorption_detail_font_size }}px;
    margin-top: 4px;
  }

  @media all and (max-width: {{ section.settings.mobile_breakpoint }}px) {
    .comparison-table-section {
      padding: {{ section.settings.mobile_section_padding }}px 15px;
    }

    .comparison-table {
      font-size: 12px;
    }

    .comparison-table thead th,
    .comparison-table tbody td {
      padding: 10px 5px;
      font-size: 12px;
    }

    .comparison-table thead th:first-child,
    .comparison-table tbody td:first-child {
      font-size: 13px;
    }

    .comparison-table .section-heading {
      font-size: {{ section.settings.mobile_heading_size }}px;
    }

    .comparison-table .product-icon img {
      max-width: 50px;
    }

    .comparison-table thead th.brand-column img {
      max-width: 80px;
    }

    .comparison-table .checkmark,
    .comparison-table .cross {
      width: 24px;
      height: 24px;
    }
  }
</style>

<div class="comparison-table-section">
  <div class="container">
    {% if section.settings.show_heading %}
      <h2 class="section-heading">{{ section.settings.heading }}</h2>
    {% endif %}

    <table class="comparison-table">
      <thead>
        <tr>
          <th></th>
          <th class="brand-column">
            {% if section.settings.brand_logo %}
              <img src="{{ section.settings.brand_logo | img_url: 'master' }}" alt="{{ section.settings.brand_name }}">
            {% else %}
              {{ section.settings.brand_name }}
            {% endif %}
          </th>
          <th>{{ section.settings.competitor_1_name }}</th>
          <th>{{ section.settings.competitor_2_name }}</th>
          <th>{{ section.settings.competitor_3_name }}</th>
        </tr>
      </thead>
      <tbody>
        {% for block in section.blocks %}
          {% case block.type %}
            {% when 'feature_row' %}
              <tr>
                <td>{{ block.settings.feature_name }}</td>
                <td>
                  {% if block.settings.brand_value_type == 'text' %}
                    {{ block.settings.brand_text }}
                  {% elsif block.settings.brand_value_type == 'check' %}
                    <span class="checkmark">
                      <svg viewBox="0 0 24 24"><path d="M9 16.17L4.83 12l-1.42 1.41L9 19 21 7l-1.41-1.41z"/></svg>
                    </span>
                  {% elsif block.settings.brand_value_type == 'cross' %}
                    <span class="cross">
                      <svg viewBox="0 0 24 24"><path d="M19 6.41L17.59 5 12 10.59 6.41 5 5 6.41 10.59 12 5 17.59 6.41 19 12 13.41 17.59 19 19 17.59 13.41 12z"/></svg>
                    </span>
                  {% elsif block.settings.brand_value_type == 'image' %}
                    <div class="product-icon">
                      <img src="{{ block.settings.brand_image | img_url: 'master' }}" alt="{{ block.settings.feature_name }}">
                    </div>
                  {% elsif block.settings.brand_value_type == 'price' %}
                    <span class="price-value" style="color: {{ block.settings.brand_price_color }};">{{ block.settings.brand_price }}</span>
                  {% elsif block.settings.brand_value_type == 'custom' %}
                    <div>
                      <strong>{{ block.settings.brand_main_text }}</strong>
                      {% if block.settings.brand_sub_text != blank %}
                        <span class="absorption-time">{{ block.settings.brand_sub_text }}</span>
                      {% endif %}
                    </div>
                  {% endif %}
                </td>
                <td>
                  {% if block.settings.competitor_1_type == 'text' %}
                    {{ block.settings.competitor_1_text }}
                  {% elsif block.settings.competitor_1_type == 'check' %}
                    <span class="checkmark">
                      <svg viewBox="0 0 24 24"><path d="M9 16.17L4.83 12l-1.42 1.41L9 19 21 7l-1.41-1.41z"/></svg>
                    </span>
                  {% elsif block.settings.competitor_1_type == 'cross' %}
                    <span class="cross">
                      <svg viewBox="0 0 24 24"><path d="M19 6.41L17.59 5 12 10.59 6.41 5 5 6.41 10.59 12 5 17.59 6.41 19 12 13.41 17.59 19 19 17.59 13.41 12z"/></svg>
                    </span>
                  {% elsif block.settings.competitor_1_type == 'image' %}
                    <div class="product-icon">
                      <img src="{{ block.settings.competitor_1_image | img_url: 'master' }}" alt="{{ block.settings.feature_name }}">
                    </div>
                  {% elsif block.settings.competitor_1_type == 'price' %}
                    <span class="price-value" style="color: {{ block.settings.competitor_1_price_color }};">{{ block.settings.competitor_1_price }}</span>
                  {% elsif block.settings.competitor_1_type == 'custom' %}
                    <div>
                      <strong>{{ block.settings.competitor_1_main_text }}</strong>
                      {% if block.settings.competitor_1_sub_text != blank %}
                        <span class="absorption-time">{{ block.settings.competitor_1_sub_text }}</span>
                      {% endif %}
                    </div>
                  {% endif %}
                </td>
                <td>
                  {% if block.settings.competitor_2_type == 'text' %}
                    {{ block.settings.competitor_2_text }}
                  {% elsif block.settings.competitor_2_type == 'check' %}
                    <span class="checkmark">
                      <svg viewBox="0 0 24 24"><path d="M9 16.17L4.83 12l-1.42 1.41L9 19 21 7l-1.41-1.41z"/></svg>
                    </span>
                  {% elsif block.settings.competitor_2_type == 'cross' %}
                    <span class="cross">
                      <svg viewBox="0 0 24 24"><path d="M19 6.41L17.59 5 12 10.59 6.41 5 5 6.41 10.59 12 5 17.59 6.41 19 12 13.41 17.59 19 19 17.59 13.41 12z"/></svg>
                    </span>
                  {% elsif block.settings.competitor_2_type == 'image' %}
                    <div class="product-icon">
                      <img src="{{ block.settings.competitor_2_image | img_url: 'master' }}" alt="{{ block.settings.feature_name }}">
                    </div>
                  {% elsif block.settings.competitor_2_type == 'price' %}
                    <span class="price-value" style="color: {{ block.settings.competitor_2_price_color }};">{{ block.settings.competitor_2_price }}</span>
                  {% elsif block.settings.competitor_2_type == 'custom' %}
                    <div>
                      <strong>{{ block.settings.competitor_2_main_text }}</strong>
                      {% if block.settings.competitor_2_sub_text != blank %}
                        <span class="absorption-time">{{ block.settings.competitor_2_sub_text }}</span>
                      {% endif %}
                    </div>
                  {% endif %}
                </td>
                <td>
                  {% if block.settings.competitor_3_type == 'text' %}
                    {{ block.settings.competitor_3_text }}
                  {% elsif block.settings.competitor_3_type == 'check' %}
                    <span class="checkmark">
                      <svg viewBox="0 0 24 24"><path d="M9 16.17L4.83 12l-1.42 1.41L9 19 21 7l-1.41-1.41z"/></svg>
                    </span>
                  {% elsif block.settings.competitor_3_type == 'cross' %}
                    <span class="cross">
                      <svg viewBox="0 0 24 24"><path d="M19 6.41L17.59 5 12 10.59 6.41 5 5 6.41 10.59 12 5 17.59 6.41 19 12 13.41 17.59 19 19 17.59 13.41 12z"/></svg>
                    </span>
                  {% elsif block.settings.competitor_3_type == 'image' %}
                    <div class="product-icon">
                      <img src="{{ block.settings.competitor_3_image | img_url: 'master' }}" alt="{{ block.settings.feature_name }}">
                    </div>
                  {% elsif block.settings.competitor_3_type == 'price' %}
                    <span class="price-value" style="color: {{ block.settings.competitor_3_price_color }};">{{ block.settings.competitor_3_price }}</span>
                  {% elsif block.settings.competitor_3_type == 'custom' %}
                    <div>
                      <strong>{{ block.settings.competitor_3_main_text }}</strong>
                      {% if block.settings.competitor_3_sub_text != blank %}
                        <span class="absorption-time">{{ block.settings.competitor_3_sub_text }}</span>
                      {% endif %}
                    </div>
                  {% endif %}
                </td>
              </tr>
          {% endcase %}
        {% endfor %}
      </tbody>
    </table>
  </div>
</div>

