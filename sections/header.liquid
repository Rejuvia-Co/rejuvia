html, body{overflow: visible!important;}

  /* Custom style  for menu open button*/

{% unless section.settings.enableSticky %}
.is-ticky .header-top{display: none;}
{% endunless %}

  .header {width: 100%; position: relative; color: {{section.settings.header_text_color}}; background: {{section.settings.header_color}}; {% if section.settings.header_border_color != 'rgba(0,0,0,0)' %}border-bottom: 1px solid {{section.settings.header_border_color}};{% else %}border: none;{% endif %} }
  .header svg {width: 20px; height: 20px;}
  .header svg path {fill: #000000/*{{section.settings.header_text_color}}*/;}

  .header .logo {line-height: 100%; width: 100%; max-width: {{section.settings.logo_max_width}}; margin-right: 24px;}

  @media screen and (min-width: 901px) {
    .header .logo {
      margin-bottom: 10px;
    }
  }

  .header .nav-wrapper, .header .navigator {width: calc(50% - {{section.settings.logo_max_width}}/2);}

  .header .logo img {width: 100%; display: block;}
  .header .logo .shop-name {font-size: 2em; text-transform: {{ settings.heading_font_transform }}; color: {{ settings.content_heading_color }}; letter-spacing: {{settings.heading_font_letter_spacing}}; line-height: 100%; }
  .header .main-content {position: relative; padding: 0em 0em 0em;}

  {%- if section.settings.drop_header or section.settings.drop_header_mobile -%}
  /**** header drop ****/
  .header-drop {position: fixed; visibility: hidden; width: 100%; box-shadow: 0 0 10px rgba(0,0,0,0.1); color: {{section.settings.drop_header_text_color}}; background: {{section.settings.drop_header_color}}; {% if section.settings.drop_header_border_color != 'rgba(0,0,0,0)' %}border-bottom: 1px solid {{section.settings.drop_header_border_color}};{% else %}border: none;{% endif %} top: 0; z-index: 999; -webkit-transform: translateY(-150%); transform: translateY(-150%);}
  .header-drop svg path {fill: {{section.settings.drop_header_text_color}};}
  .header-drop.scroll {z-index: 2000; visibility: visible; -webkit-transform: translateY(0); transform: translateY(0);}
  .header-drop .main-content {position: relative; padding: 0em 0; }
  .header-drop .logo {max-width: {{section.settings.drop_logo_max_width}};}
  .header-drop .utility-bar {margin: 0;}
  .header-drop .top-utils a:not(:last-child) {margin-right: 2em;}
  .header-drop .top-utils svg {margin-right: 0;}
  .header-drop .top-utils span:not(.count) {border: 0; clip: rect(0 0 0 0); height: 1px; margin: -1px; overflow: hidden; padding: 0; position: absolute; width: 1px;}
  .header-drop .top-utils .logout {display: none;}
  .header-drop .main-menu {margin: 0 2em;}
  .header-drop .main-menu .item:hover {border-bottom: 2px solid #979797;}
  .header-drop .main-menu .item a {padding: 1.2em 0; font-weight: 500;}
  .header-drop .navigator-right ul {padding: 1.5em 0;}
  {%- endif -%}

  /**** menu ****/
  .main-menu .item {position: relative; font-family: inherit; margin: 0; letter-spacing: {{section.settings.menu_letter_spacing}}; text-transform: {{ section.settings.menu_text_transform }}; font-size: {{section.settings.menu_font_size}}; font-weight: {{section.settings.menu_font_weight}}; margin-right: 0; border-bottom: 2px solid transparent;text-decoration: none;}
  .main-menu .item a {display: flex!important; display: -webkit-flex!important; align-items: center!important; padding: 34px 0px 34px 50px;}
  .main-menu .item .active-menu-item {
     /*text-decoration: underline;*/
  }
  .main-menu .item a:hover {color:#EAC78B;text-decoration: none;}
  .main-menu .item a svg {display: none !important;}
  .main-menu .item:first-child a {padding-left: 0;}
  .main-menu .item:last-child a {padding-right: 0;}
  .main-menu svg.icon {display: none; margin-left: 0.5em;}

  /* dropdown menus */
  .main-menu .submenu {display: none; font-size: 1em; background: {{ section.settings.dropdown_bg_color }}; color: #000;}
  .main-menu .has-dropdown > a {position: relative;}
  .main-menu .has-dropdown > a > svg.icon {display: inline-block;}
  .main-menu .has-dropdown .submenu {top: 100%; left: -0.5em; padding: .3em 1em .3em 0; position: absolute; z-index: 30; margin-top: 1em;
  -webkit-box-shadow: -2px 3px 9px 0px rgba(60,60,60,1);
  -moz-box-shadow: -2px 3px 9px 0px rgba(60,60,60,1);
  box-shadow: -2px 3px 9px 0px rgba(60,60,60,1);
  -webkit-transition: padding 0.2s ease-out; -moz-transition: padding 0.2s ease-out; -o-transition: padding 0.2s ease-out; -ms-transition: padding 0.2s ease-out; transition: padding 0.2s ease-out; box-shadow:none; border-top:1px solid #ddd;
  }
  .main-menu .has-dropdown .submenu.mega-menu,
  body.scroll .main-menu .has-dropdown .submenu { margin-top: 0;}
  .main-menu .has-dropdown .item {font-size: 1em; text-align: left; display: block; margin: 0; }
  .main-menu .has-dropdown .item:last-child {border-bottom: 0px;}
  .main-menu .has-dropdown .item a {position: relative; margin: 0; min-width: 270px; padding: 0.3em 1.5em; border: none; padding-right: 0px; color: #000; font-weight: 400; text-transform: capitalize; }
  .main-menu .has-dropdown .item a:hover {color:#fff;text-decoration: underline;}
  .main-menu .has-dropdown .has-dropdown svg.icon {position: absolute; right: 20px; top: 50%; -webkit-transform: translateY(-50%); transform: translateY(-50%);}

  /* level 3 */
  .main-menu .has-dropdown .submenu .has-dropdown .submenu {top: 0; left: 100%;}

  {%- if settings.mega_menu_width == "adjust" -%}
  .mega-menu .column {background: {{ section.settings.dropdown_bg_color }};}
  {%- endif -%}
  {%- if settings.mega_menu_width == "container" -%}
  .mega-menu .mega-menu-content {background: {{ section.settings.dropdown_bg_color }};}
  {%- endif -%}
  {%- if settings.mega_menu_width == "adjust" and settings.mega_menu_position == "parent" -%}
  .mega-menu {background: {{ section.settings.dropdown_bg_color }} !important;}
  {%- endif -%}

  /* menu mobile */

  .btn-open-mobile-menu {display: flex; align-items: left; justify-content: left; margin-top: 3px!important;}
  .btn-open-mobile-menu svg {width: 32px; height: 32px;}


  .menu-mobile-button {width: 2.5em; font-size: 18px; position: absolute; top: 0; right: 0; height: 100%; background: {{settings.mobile_menu_button_color}}; fill: {{settings.mobile_menu_button_icon}}; display: flex; display: -webkit-flex; justify-content: center; align-items: center; }
  .menu-mobile-button svg path {fill: inherit !important;}

  .header-open {
    background-color: #ffffff !important;
    transition: background-color 0.7s ease-in-out;
}

  #header > .container{
    /* transition: background-color 0.7s ease-in-out; */
  }

  @media(min-width: 500px){
    #header > .container{
      background-color: #ffffff !important;
    }
  }


  #menu-mobile {background: transparent; display: none; outline: none; visibility: hidden; width: 400px;   right: -415px; overflow: hidden;  background-color: {{settings.mobile_menu_bg_color}}; position: absolute; top: 55px;  z-index: -1; }
  #menu-mobile.open {right: 0.1em; visibility: visible;}
  #menu-mobile .wrapper {display: flex; display: -webkit-flex; flex-direction: column; height: 100%;}
  #menu-mobile .head {margin-top: 0px; box-sizing: border-box; padding: .3em 2rem; line-height: 100%; position: relative; margin-bottom: 0; background: {{settings.mobile_menu_title_bg_color}}; border-bottom: 0px solid {{settings.mobile_menu_borders_color}};}
  #menu-mobile .logo img {max-width: 70%;}
  /**/
  #menu-mobile .head {position: absolute; right: 0; width: 40%; height: 100vh;}
  #menu-mobile .head .title {text-transform:{{settings.mobile_menu_title_transform}}; line-height: 100%; margin: 0; padding: 0; border: none; font-weight: {{settings.mobile_menu_title_weight}}; color: {{settings.mobile_menu_title_color}};  }
  #menu-mobile .custom-nav-links-container a {
    color: {{settings.mobile_menu_text_color}};
    display: block;
    font-size: 1.5rem;
    padding: 1.5rem 0;
    border-bottom: 1px solid black;
  }

  #menu-mobile .custom-nav-links-container a:last-of-type {
    border-bottom: none;
  }

  #menu-mobile .close {
    cursor: pointer;

    min-height: 32px !important;
    min-width: 32px !important;

  }
  #menu-mobile .close svg {
    max-height: 100%;
    max-width: 100%;
  }
  #menu-mobile .close svg path {
    fill: {{settings.mobile_menu_title_color}};
  }
  #menu-mobile .content {width: 100%; background: #fff; overflow-y: auto; flex: 1;}
  #menu-mobile ul {list-style-type: none;}
  #menu-mobile .menu-mobile {font-size: {{settings.mobile_menu_font_size}}; }
  #menu-mobile .item-link {font-size: 16px; text-transform: normal; font-weight: 500;}
  #menu-mobile .submenu a {font-size: 18px; text-transform: none;}
  #menu-mobile .menu-mobile .item {position: relative;  margin-top:.5em; padding: 1em 0.9em; text-transform: {{settings.mobile_menu_transform}};}
  #menu-mobile .menu-mobile .item:last-child {border-bottom: 0px;}
  #menu-mobile .menu-mobile .item svg {cursor: pointer; position: absolute; top: 0; right: -0.5em; font-size: 1.8em; box-sizing: content-box; padding: 0.5em;}
  #menu-mobile .menu-mobile .item svg path {fill: {{settings.mobile_menu_arrows_color}};}
  #menu-mobile .menu-mobile .item.active > a + svg {-ms-transform: rotate(180deg); -webkit-transform: rotate(180deg); transform: rotate(180deg);}
  #menu-mobile .icon {display: none!important;}
  #menu-mobile .menu-mobile .submenu {display: none; padding: 1em 0 0 0; margin-bottom: -1em;}
  #menu-mobile .menu-mobile .submenu .item:first-child {border-top: 1px solid {{settings.mobile_menu_borders_color}};}
  #menu-mobile .top-utils {position: static; display: inline-block;}
  #menu-mobile .top-utils .svg {font-size: 1.2em; margin-top: 3em; }
  #menu-mobile .foot {padding: 2rem; padding-top: 0;}
  #menu-mobile .search-form {position: relative;}
  #menu-mobile .search-form .input-search {padding-right: 3em;}
  #menu-mobile .search-form button {position: absolute; font-size: 1.3em; top: 50%; right: 1em; line-height: 100%; -webkit-transform: translateY(-50%); transform: translateY(-50%);}
  #menu-mobile .social-links {margin-top: 2rem; justify-content: flex-start;}
  #menu-mobile .social-links a {margin-right: 1em;}
  #menu-mobile .social-links a:last-child {margin-right: 0;}
  #menu-mobile .menu-mobile .item { font-weight: 600; font-size: 1.1em; text-transform: none;}
  #menu-mobile .menu-mobile .submenu .item { font-weight: 300; text-transform: normal; font-size: 1.3em; margin:0;}
  #menu-mobile .menu-mobile .submenu .item .submenu .item { font-weight: 300; text-transform: capitalize; font-size: .9em; }
  #menu-mobile .menu-mobile .item svg { font-size: 1em; top: 6px; }
  #menu-mobile .menu-mobile .item svg path { fill: #444; }
  #menu-mobile .menu-mobile .submenu .item svg { font-size: .8em; }
  #menu-mobile .menu-mobile .submenu .item:first-child { border: none; }
  #menu-mobile .head { background: transparent; border-bottom: none; padding: 0; }
  #menu-mobile .head .title { width: 80%; background: #fff; height: 0; }

  {%- if settings.mobile_menu_position == "left" -%}
  #menu-mobile {right: auto; left: -415px;}
  #menu-mobile.open {right: auto; left: 0;}
  {%- endif -%}

  #menu-mobile .menu-item {display: flex; justify-content:flex-start; align-items: center;}
  #menu-mobile .menu-item .menu-item-title {margin: 0;}
  #menu-mobile .menu-item .item-subtitle {font-size: 0.75em;}
  #menu-mobile .menu-item img {max-width: 25%;}

  #menu-mobile .content { display: flex;
  flex-direction: column;
  justify-content: flex-start;
  height: 100%; }
  #menu-mobile .menu-mobile {
    height: calc(100% - 100px);
    overflow-y: scroll;
  }
  #menu-mobile .foot {
  padding: 0 1em;
  color: #fff;
  height: 50px;}
  #menu-mobile .foot .btn {
    color: #ffffff;
    /* height: 100%; */
    text-transform: capitalize;
    text-align: center;
    width: 100%;
  }
  #menu-mobile .foot .btn:hover {color: #fff;}
  .header .topbadge {font-size: 12px; line-height: 1.5em; margin-left: 0; width: 1.5em; height: 1.5em; position: relative; right: 0; top: -.7em; display: inline-block;}


  #menu-mobile header {
    display: flex;
    justify-content: center;
    /*margintop : 15px*/
    margin-top: 0;
  }

  #menu-mobile header .logo img {
    max-width: 100%;
  }

  #menu-mobile nav,
  #menu-mobile footer {
    padding: 0 calc(2rem + 15px);
  }

  #menu-mobile footer {
    display: flex;
    justify-content: space-between;
  }

  #menu-mobile a {
    font-size: 1.5rem;
  }

  #menu-mobile a.mobile-menu-footer-link-account {
        border: 1px solid black;
    padding: 15px 2rem 12px;
    border-radius: 32px;
    position: relative;
    top: -0.5rem;
    font-size: 17px;
    line-height: 1;
  }

  #menu-mobile a.mobile-menu-footer-link-contact {
    font-variant: small-caps;
    text-transform: lowercase;
    padding-bottom: 2px;
    border-bottom: 1px solid black;
    height: fit-content;
  }



  {%- assign utilityCount = 0 -%}
  {%- for block in section.blocks -%}
  {%- if block.type == 'utility_bar' and utilityCount == 0 -%}
      {%- assign utilityCount = utilityCount | plus: 1 -%}
        /**** utility bar ****/
        .utility-bar {line-height: 100%; position: relative; overflow: hidden; padding: 15px 0; text-align: center; background: #171512; color: white; {% if template == 'index' %}margin-top:0em;{% endif %}}
        .utility-bar .container {padding: 0em 0em; max-width: 100%;}
        .utility-bar p {margin: 0; color: inherit;}
        .utility-bar .close {position: absolute; right: 0em; top: 0%; font-size: 0px; -webkit-transform: translateY(-50%);  transform: translateY(-50%);}
        .utility-bar .close svg {display: block;}
        .utility-bar .close svg path {fill: inherit;}



    {%- endif -%}
  {%- endfor -%}

  {%- assign topUitlsCount = 0 -%}
  {%- for block in section.blocks -%}
  {%- if block.type == 'top_utils' and topUitlsCount == 0 -%}
      {%- assign topUitlsCount = topUitlsCount | plus: 1 -%}
      .utils-wrap {padding: 0em 0; font-size: {{block.settings.top_utils_size}}; font-weight: {{block.settings.top_utils_weight}}; text-transform: {{block.settings.top_utils_transform}};}
      .utils-wrap * {line-height: 100%;}
      .utils-wrap .social-links {margin-right: 3em;}
      .utils-wrap .social-links a:not(:last-child) {margin-right: 1.5em;}
      .utils-wrap .social-links svg {display: block;}
      .top-utils a {margin-right: 1.5em; display: flex; display: -webkit-flex; align-items: center;}
      .top-utils a:last-child {margin: 0;}
      .top-utils svg {margin-right: 0.8em; font-size: 1.1em;}
      .top-utils .count {padding-left: 0.5em;}
      {%- if block.settings.show_newsletter or block.settings.show_search -%}
      /**** full screen search and mailing ****/
        .search-mailing {position: fixed; z-index: 4000; width: 100%; height: 100%; display: none; top: 0; left: 0; background: rgba({{block.settings.search_mailing_bg}});}
        .search-mailing .close {position: fixed; display: none; z-index: 99997; top: 30px; right: 30px; font-size: 35px; cursor: pointer}
        .search-mailing .form-wrap {position: fixed; z-index: 99997; width: 80%; max-width: 600px; left: 50%; top: 45%; -webkit-transform: translateX(-50%) translateY(-50%);  transform: translateX(-50%) translateY(-50%);}
        .search-mailing svg path {fill: {{block.settings.search_mailing_text}};}
        .search-mailing form {display: none; width: 100%;}
        .search-mailing form button {cursor: pointer; position: absolute; font-size: 35px; right: 0; top: 50%; -webkit-transform: translateY(-50%); transform: translateY(-50%);}
        .search-mailing form input:not(.btn), .search-mailing form input:not(.btn):focus {color: {{block.settings.search_mailing_text}}; font-size: 50px; padding: 5px 40px 5px 0; width: 100%; border: none; border-bottom: 1px solid {{block.settings.search_mailing_text}}; background: transparent;}
        .search-mailing form ::-webkit-input-placeholder {color: {{block.settings.search_mailing_text}};}
        .search-mailing form :-moz-placeholder {color: {{block.settings.search_mailing_text}}; opacity: 1;}
        .search-mailing form ::-moz-placeholder {color: {{block.settings.search_mailing_text}}; opacity: 1;}
        .search-mailing form :-ms-input-placeholder {color: {{block.settings.search_mailing_text}};}
        .search-mailing .success-msg {margin: 0; color: {{block.settings.search_mailing_text}}; font-size: 50px; padding: 0; width: 100%; line-height: 100%; border: 0; background: transparent;}
      .search-mailing .error-msg {position: absolute; top: 120%; left: 0;}
      {%- endif -%}
      @media all and (max-width: 900px){
        .top-utils {padding-right: 2.5em;}
        .top-utils svg {margin: 0;}
        .top-utils span:not(.count) {border: 0; clip: rect(0 0 0 0); height: 1px; margin: -1px; overflow: hidden; padding: 0; position: absolute; width: 1px;}
        {%- if block.settings.show_newsletter or block.settings.show_search -%}
          .search-mailing svg.icon-close {font-size: 30px; top: 15px; right: 15px}
          .search-mailing .form svg {font-size: 25px;}
          .search-mailing .form input {font-size: 40px;}
        {%- endif -%}
      }
      @media all and (max-width: 600px){
        .top-utils .newsletter {display: none;}
        {%- if block.settings.show_newsletter or block.settings.show_search -%}
          .search-mailing .form input {font-size: 20px;}
          .search-mailing .form svg {font-size: 18px;}
        {%- endif -%}
      }
    {%- endif -%}
  {%- endfor -%}

  @media all and (min-width: 901px){
    {%- if section.settings.over_carousel -%}
      .hero-carousel {margin-top: -25px;}
      #header.over-carousel-desktop {background: {% if section.settings.over_carousel_bg_color == blank %}transparent{% else %}{{section.settings.over_carousel_bg_color}}{% endif %}; color: {{section.settings.over_carousel_text_color}}; position: absolute; z-index: 100;}
      body:not(.scroll) #header.over-carousel-desktop {border-color: {% if section.settings.over_carousel_bg_color == blank %}transparent{% else %}{{section.settings.over_carousel_bg_color}}{% endif %} }
      #header.over-carousel-desktop :not(.utility-bar) a:not(:hover) svg path {fill: {{section.settings.over_carousel_text_color}};}
    {%- endif -%}
    {%- if section.settings.behavior == "fixed" -%}
      #header.fixed-desktop {position: fixed; z-index: 100; top: 0; left: 0;}
      body.scroll .header .main-content {padding-top: 0; padding-bottom: 0;}
    {%- endif -%}
    {%- if section.settings.over_carousel and section.settings.behavior == "fixed" -%}
      #header.over-carousel-desktop.fixed-desktop + .header-spacer {display: none !important;}
    {%- endif -%}
    {%- if section.settings.change_header_colors_scroll -%}
      #header.change-scroll-desktop {-webkit-transition: all 0.5s ease-out; -moz-transition: all 0.5s ease-out; -o-transition: all 0.5s ease-out; -ms-transition: all 0.5s ease-out; transition: all 0.5s ease-out;}
      #header.change-scroll-desktop a {-webkit-transition: none; -moz-transition: none; -o-transition: none; -ms-transition: none; transition: none;}
      .scroll #header.change-scroll-desktop {background: {{section.settings.scroll_bg_color}} !important; color: {{section.settings.scroll_text_color}} !important;}
      .scroll #header.change-scroll-desktop :not(.utility-bar) a:not(:hover) svg path {fill: {{section.settings.scroll_text_color}} !important;}
    {%- endif -%}

  }

  /* fixes issue with header positioning */
  @media all and (min-width: 601px) {
    .header-spacer {height: 100px;}
  }
  @media all and (max-width: 600px) {
    .header .main-content {padding-top: 1em; padding-bottom: 1em;}
  }


  /* iPad Pro Portrait */
 @media only screen
 and (min-device-width: 1024px)
 and (max-device-height: 1366px)
 and (-webkit-min-device-pixel-ratio:2)
 and (orientation: portrait) {
   .header-spacer {height: 130px;}
 }
 /* iPad Pro Landscape */
 @media only screen
 and (min-device-width: 1024px)
 and (max-device-height: 1366px)
 and (-webkit-min-device-pixel-ratio: 2)
 and (orientation: landscape) {
   .header-spacer {height: 130px;}
 }

  @media all and (max-width: 900px) {
    #menu-mobile .menu-mobile .item > .submenu > .item > a:hover {text-decoration: underline;}

    {%- unless section.settings.drop_header_mobile -%}
    .header-drop {display: none;}
    {%- endunless -%}
    .header .topbadge {display: none !important;}

    .header {color: {{section.settings.header_text_color_mobile}}; background: {{section.settings.header_color_mobile}}; {% if section.settings.header_border_color_mobile != 'rgba(0,0,0,0)' %}border-bottom: none {{section.settings.header_border_color_mobile}};{% else %}border: none;{% endif %} }
    /*.header :not(.menu-mobile-button) svg path {fill: {{section.settings.header_text_color_mobile}};}*/
    .header .container {padding: 0;}
    .header .main-content {position: relative; padding: 0.5em 14px 0.5em 20px;}
    .header .navigator-mobile {width: 100%;}
    .header .navigator-mobile .wishlist-link svg {transform: translateY(1px); margin: 0 0.2em;}
    .header .logo {max-width: {{section.settings.logo_max_width_mobile}};}
    .header .social-links {display: none !important;}
    .utils-wrap {padding: 0;}
    .utility-bar {margin-top: 0;}

    #menu-mobile {display: block;}

    {%- if section.settings.over_carousel_mobile -%}
      #header.over-carousel-mobile {background: {% if section.settings.over_carousel_bg_color_mobile == blank %}transparent{% endif %}; color: {{section.settings.over_carousel_text_color_mobile}}; position: absolute; z-index: 100;}
      body:not(.scroll) #header.over-carousel-mobile {border-color: {% if section.settings.over_carousel_bg_color_mobile == blank %}transparent{% endif %} }
      #header.over-carousel-mobile :not(.utility-bar) a:not(:hover) svg path {fill: {{section.settings.over_carousel_text_color_mobile}};}
    {%- endif -%}
    {%- if section.settings.behavior_mobile == "fixed" -%}
      #header.fixed-mobile .header-top {position: fixed; z-index: 100;}
    #header.fixed-mobile > .container{
    	    position: relative;
      top: 37px;}
    .templateIndex #header.fixed-mobile > .container{
    	    position: absolute;
    top: 45px;
    width: 100%;
    z-index: 1;
    }
    {%- endif -%}
    {%- if section.settings.over_carousel_mobile and section.settings.behavior_mobile == "fixed" -%}
      #header.over-carousel-mobile.fixed-mobile + .header-spacer {display: none !important;}
