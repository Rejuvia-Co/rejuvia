        {%- endif -%}
            <div class="desktops item_img{% if block.settings.image != blank %} lazyload{% unless forloop.first == true %} lazypreload{% endunless %}{% endif %}" {% if block.settings.image != blank %} {% if forloop.first == true %}style="background-image: url({{ block.settings.image | img_url: '300x' }});"{% endif %} data-bgset="{% include 'lazy_src', image: block.settings.image %}"{% endif %}></div>   
             <div class="mobiles item_img{% if block.settings.image_mb != blank %} lazyload{% unless forloop.first == true %} lazypreload{% endunless %}{% endif %}" {% if block.settings.image_mb != blank %} {% if forloop.first == true %}style="background-image: url({{ block.settings.image_mb | img_url: '300x' }});"{% endif %} data-bgset="{% include 'lazy_src', image: block.settings.image_mb %}"{% endif %}></div>   
