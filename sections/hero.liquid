            #shopify-section-{{ section.id }} .block-{{ block.id }} .captions .text {color: {{block.settings.text_color_mobile}}; font-size:{{block.settings.fontSizem}}px; margin-bottom: 0; line-height: 140%; letter-spacing: normal; }
