<style>
 
  .hero-carousel {overflow: hidden; box-sizing: initial;}
  .hero-carousel.slick-pending {display: block; max-height: 90vh;}
  .hero-carousel.slick-initialized {max-height: none;}
  .hero-carousel .slick-list {padding: 0;}
  .hero-carousel .item_img {background-repeat: no-repeat; background-position: top; background-size: cover; }
  .hero-carousel .item {position: relative;}
  .hero-carousel .overlay {padding: 0; position: absolute; z-index: 5; top: 0; left: 0; width: 100%; height: 100%; }
  .hero-carousel .overlay:before {position: absolute; left: 0; top: 0; width: 100%; height: 100%;}
  .hero-carousel .content {position: relative; height: 100%;}
  .hero-carousel .captions {position: relative;}
  .hero-carousel .title {color: inherit; -webkit-animation-delay: 0.2s; animation-delay: 0.2s; padding:0em;}
  .hero-carousel .text {color: inherit; -webkit-animation-delay: 0.4s; animation-delay: 0.4s;}
  .hero-carousel .text p {color: inherit;}
  .hero-carousel .btn {-webkit-animation-delay: 0.6s; animation-delay: 0.6s;}
  .hero-carousel .slick-slide {margin: 0 !important; }
  .hero-carousel .slick-arrow {opacity: 0; filter: alpha(opacity=0);}
  .isNotMobile .hero-carousel:hover .slick-arrow.left-arrow {opacity: 1; filter: alpha(opacity=100); left: 30px;}
  .isNotMobile .hero-carousel:hover .slick-arrow.right-arrow {opacity: 1; filter: alpha(opacity=100); right: 30px;}
  .hero-carousel .featured-img {-webkit-animation-delay: 0.8s; animation-delay: 0.8s; width: 20%; height: 100%; position: absolute; right: 10%; top: 0; background-size: contain; background-repeat: no-repeat;}
  
  @media all and (min-width: {{settings.breakpoint_tablet | plus: 1}}px){
  	.hero-carousel .container {padding: 0 120px;}
    {%- unless section.settings.full_width -%}
      #shopify-section-{{ section.id }} {box-sizing: content-box; max-width: {{settings.container_width}}; margin: auto; padding: 0 1.5rem;}
    {%- endunless -%}
  }
  
  #shopify-section-{{ section.id }} .hero-carousel.normal-desktop .item_img {padding-top: {{section.settings.img_height}};}
  #shopify-section-{{ section.id }} .hero-carousel .slick-arrow {color: {{section.settings.arrow_color}};}
  #shopify-section-{{ section.id }} .hero-carousel .slick-arrow path {fill: {{section.settings.arrow_color}};}
  #shopify-section-{{ section.id }} .isNotMobile .hero-carousel .slick-arrow:hover path {fill: {{section.settings.arrow_hover_color}};}
  #shopify-section-{{ section.id }} .hero-carousel .slick-dots {position: absolute; bottom: 2em; margin: 0;}
  #shopify-section-{{ section.id }} .hero-carousel .slick-dots li button {background: {{section.settings.dots_color}}; border: 0px solid {{section.settings.dots_color}};}
  #shopify-section-{{ section.id }} .hero-carousel .slick-dots li.slick-active button {background: {{ section.settings.dots_active_color }}; border-color: {{ section.settings.dots_active_color }};}
  
  @media all and (max-width: {{settings.breakpoint_tablet}}) {
    .hero-carousel.captions_below .overlay {position: static; height: auto;}
    .hero-carousel.captions_below .slick-track {display: -webkit-box !important; display: -moz-box !important; display: -ms-flexbox !important; display: -webkit-flex !important; display: flex !important; }
    .hero-carousel.captions_below .slick-track:before, .hero-carousel.captions_below .slick-slider .slick-track:after {display: none;}
    .hero-carousel.captions_below .slick-track .slick-slide {height: auto;}
    .hero-carousel.captions_below .content {position: static;}
    .hero-carousel.captions_below .featured-img {width: 50% !important; padding-top: 50% !important; height: auto; top: 0 !important; right: auto !important; left: 50% !important; margin-left: -25%;}
    .hero-carousel.captions_inside .featured-img {position: static; margin: 0 auto; width: 35% !important; height: auto; padding-top: 35%;}
    
    {%- unless section.settings.full_width_mobile -%}
      #shopify-section-{{ section.id }} {box-sizing: content-box; padding: 0.5em 0.75rem;}
    {%- endunless -%}
    
    #shopify-section-{{ section.id }} .hero-carousel.normal-mobile .item_img {padding-top: {{section.settings.img_height_mobile}}; background-position: center;}
  }
  
  
  #text-review{
	display:flex;padding-left: 0px;margin-bottom: -14px;  
  }
  
  @media(max-width:767px){
	   #text-review{justify-content: center;
	padding-left: 0px;margin-bottom: 0px; margin-top:74px;
  }
  }

  @media(min-width:900px){
    .hero-carousel{
      /* min-height: 800px; */
      height: auto;
    }
    .hero-carousel .desktops{
      min-height: 650px;
      height: auto;
    }

    .hero-carousel .item{
      min-height: 650px;
      height: auto;
    }

    {% comment %} .hero-carousel .overlay-1{
      min-height: 650px;      
    } {% endcomment %}
  }
</style>

